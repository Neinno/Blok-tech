{{>header}}

<section class="match">
    <h1>Jouw auto's</h1>

    {{#each cars}}
    <article>
        <section class="main">
            <a href="/{{_id}}/{{slug}}"><h2>{{merk}}</h2></a>
            <p>Kenteken: {{kenteken}}</p>
        </section>
        <section class="info">
            <p>Type: {{type}}</p>
            <p>Kleur: {{kleur}}</p>
        </section>
    </article>
    {{/each}}

</section>